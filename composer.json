{
  "name": "giltorres/quotingfast-brain",
  "type": "project",
  "description": "QuotingFast Lead Management Platform",
  "keywords": ["laravel", "php", "lead-management"],
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "App\\": "brain/app/",
      "Database\\Factories\\": "brain/database/factories/",
      "Database\\Seeders\\": "brain/database/seeders/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "cd brain && composer install --no-dev --optimize-autoloader",
      "cd brain && php artisan config:cache",
      "cd brain && php artisan route:cache"
    ],
    "post-autoload-dump": [
      "cd brain && php artisan package:discover --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}